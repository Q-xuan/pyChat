    <Loggers>

        <logger name="OPEN_AND_CLOSE" level="debug" additivity="true">
            <appender-ref ref="OPEN_AND_CLOSE"/>
        </logger>

        <logger name="MYSQL_CONNECTIONS" level="debug" additivity="true">
            <appender-ref ref="MYSQL_CONNECTIONS"/>
        </logger>

        <logger name="fullGc" level="info" additivity="true">
            <appender-ref ref="FULL_GC"/>
        </logger>

        <logger name="TASK_TIME" level="debug" additivity="true">
            <appender-ref ref="TASK_TIME"/>
        </logger>

        <logger name="MONITOR"   level="debug" additivity="true">
            <appender-ref ref="MONITOR"/>
        </logger>

        <logger name="com.alibaba.druid.pool.DruidDataSourceStatLoggerImpl" level="debug" additivity="false">
            <appender-ref ref="DATASOURCE_STAT"/>
