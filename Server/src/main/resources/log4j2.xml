        <logger name="COMMAND_TIME" level="debug" additivity="false">
            <appender-ref ref="COMMAND_TIME"/>
        </logger>

        <logger name="REQUIRE_AND_RESPONSE" level="debug" additivity="false">
            <appender-ref ref="REQUIRE_AND_RESPONSE"/>
            <appender-ref ref="CONSOLE"/>
        </logger>

        <logger name="THREAD" level="debug" additivity="true">
            <appender-ref ref="THREAD"/>
