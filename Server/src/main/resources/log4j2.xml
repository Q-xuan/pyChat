        <RollingFile name="REQUIRE_AND_RESPONSE" fileName="${LOG_HOME}/${REQUIRE_AND_RESPONSE_LOG_FILE_NAME}" filePattern="${LOG_HOME}/${REQUIRE_AND_RESPONSE_LOG_FILE_NAME}.${FILE_PATTERN}" append="true">
