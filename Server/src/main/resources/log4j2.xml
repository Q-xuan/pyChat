            	<SizeBasedTriggeringPolicy size="${FILE_SIZE}"/>
            </Policies>
            <DefaultRolloverStrategy max="20"/>
        </RollingFile>

        <RollingFile name="THREAD" fileName="${LOG_HOME}/${THREAD_LOG_FILE_NAME}" filePattern="${LOG_HOME}/${THREAD_LOG_FILE_NAME}.${FILE_PATTERN}" append="true">
            <Filters>
            	<ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
