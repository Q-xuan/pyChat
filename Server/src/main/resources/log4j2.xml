         <RollingFile name="DATASOURCE_STAT" fileName="${LOG_HOME}/${DATASOURCE_STAT_LOG_FILE_NAME}" filePattern="${LOG_HOME}/${DATASOURCE_STAT_LOG_FILE_NAME}.${FILE_PATTERN}" append="true">
            <Filters>
            	<ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>
            	<ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="debug" onMatch="ACCEPT" onMismatch="NEUTRAL"/>
            </Filters>
            <PatternLayout pattern="${LAYOUT_PATTERN}"/>
            <Policies>
            	<!-- TimeBasedTriggeringPolicy modulate="true" interval="24" /-->
            	<SizeBasedTriggeringPolicy size="${FILE_SIZE}"/>
            </Policies>
            <DefaultRolloverStrategy max="20"/>
        </RollingFile>

         <RollingFile name="COMMAND_TIME" fileName="${LOG_HOME}/${COMMAND_TIME_LOG_FILE_NAME}" filePattern="${LOG_HOME}/${COMMAND_TIME_LOG_FILE_NAME}.${FILE_PATTERN}" append="true">
            <Filters>
<!--            	<ThresholdFilter level="warn" onMatch="DENY" onMismatch="NEUTRAL"/>-->
            	<ThresholdFilter level="error" onMatch="DENY" onMismatch="NEUTRAL"/>
                <ThresholdFilter level="debug" onMatch="ACCEPT" onMismatch="NEUTRAL"/>
            </Filters>
            <PatternLayout pattern="${LAYOUT_PATTERN}"/>
            <Policies>
            	<!-- TimeBasedTriggeringPolicy modulate="true" interval="24" /-->
